keywords = {Adolescent,Exercise,Female,Health Behavior,Health Care Surveys,Healthy People Programs,Humans,Logistic Models,Male,Physical Education and Training,Prevalence,Students,Students: statistics {\&} numerical data,Television,United States,United States: epidemiology},
month = {jan},
