number = {99},
pages = {1--188},
pmid = {26616119},
title = {{Behaviour change techniques: the development and evaluation of a taxonomic method for reporting and describing behaviour change interventions (a suite of five studies involving consensus methods, randomised controlled trials and analysis of qualitative da}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26616119},
volume = {19},
year = {2015}
}
