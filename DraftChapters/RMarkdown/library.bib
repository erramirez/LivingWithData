file = {:Users/eramirez/Documents/Mendeley Desktop/Relationships of occupational and non-occupational physical activity to abdominal obesity. - 2011 - Steeves et al.pdf:pdf},
issn = {1476-5497},
journal = {International journal of obesity (2005)},
keywords = {abdominal adiposity,high-risk,nhanes,waist circumference},
month = {mar},
