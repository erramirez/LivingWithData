keywords = {Adolescent,Body Temperature,Child,Energy Metabolism,Exercise,Exercise: physiology,Habits,Health Behavior,Health Status Indicators,Heart Rate,Humans,Models, Biological,Monitoring, Ambulatory,Monitoring, Ambulatory: instrumentation,Motor Activity,Nonlinear Dynamics,Questionnaires,Reproducibility of Results},
month = {sep},
