title = {{Regression Calibration for Dichotomized Mismeasured Predictors.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2743435{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {5},
