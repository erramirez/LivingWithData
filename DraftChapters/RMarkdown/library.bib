@article{Must2009,
abstract = {Sedentary behavior and sleep may be working in concert to increase the likelihood of a child becoming overweight, but in paradoxical ways. Reduction of sedentary behavior (that is, media screen time) has been extensively researched and touted as an intervention target. Inadequate sleep as a putative risk factor for obesity is only beginning to be explored. In this paper, we review the current state of research regarding these factors, and describe the existing evidence and mechanisms proposed to explain these relationships. Whereas the association between weight and sedentary behavior has been consistently shown in observational studies, effect sizes are small, and multiple mechanisms appear to be operating. Recent cross-sectional and longitudinal evidence suggests a link between short sleep duration and weight. Possible mechanisms here include direct metabolic effects as well as indirect behavioral pathways, including the presence of electronic media in children's bedrooms. Measurement issues present a challenge to both areas of research. Prospective studies that include more accurate measures of both sedentary behavior and of sleep will be needed to clarify causal pathways.},
author = {Must, a and Parisi, S M},
doi = {10.1038/ijo.2009.23},
