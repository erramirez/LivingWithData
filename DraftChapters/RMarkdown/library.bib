journal = {Contemporary clinical trials},
keywords = {Adolescent,Adolescent Behavior,Age Factors,Child,Cross-Sectional Studies,Exercise,Female,Health Promotion,Humans,Life Style,Randomized Controlled Trials as Topic,Randomized Controlled Trials as Topic: methods,Research Design,Schools},
