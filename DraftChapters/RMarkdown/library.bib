issn = {1476-6256},
journal = {American Journal of Epidemiology},
keywords = {Adult,Aged,Breast Neoplasms,Breast Neoplasms: prevention {\&} control,Carotenoids,Carotenoids: administration {\&} dosage,Diet Records,Female,Humans,Middle Aged,Models,Nutrition Assessment,Questionnaires,Randomized Controlled Trials as Topic,Reproducibility of Results,Retrospective Studies,Statistical},
month = {oct},
