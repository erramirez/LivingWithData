issn = {1932-6203},
journal = {PLoS ONE},
keywords = {Environmental epidemiology,Epidemiology,Medicine,Nutrition,Obesity,Preventive medicine,Public Health and Epidemiology,Public health,Research Article,job,occupations},
mendeley-tags = {job,occupations},
month = {may},
