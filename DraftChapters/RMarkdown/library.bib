issn = {1530-0315},
journal = {Medicine and science in sports and exercise},
keywords = {Adolescent,Anthropometry,Child,Cross-Sectional Studies,Energy Metabolism,Energy Metabolism: physiology,Exercise,Female,Health Behavior,Humans,Male,Monitoring, Physiologic,Monitoring, Physiologic: instrumentation,Obesity,Religion,Saskatchewan,Sedentary Lifestyle,Social Change,Technology,Young Adult},
