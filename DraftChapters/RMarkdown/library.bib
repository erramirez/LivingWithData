isbn = {0009-921X},
journal = {Clinical Orthopaedics and Related Research},
number = {7},
pages = {2042--2048},
title = {{Smartphone Apps for Orthopaedic Surgeons}},
url = {<Go to ISI>://WOS:000291484700037},
volume = {469},
year = {2011}
