journal = {Preventive medicine},
keywords = {Adult,California,Cross-Sectional Studies,Female,Health Behavior,Humans,Life Style,Middle Aged,Nutrition Surveys,Obesity,Odds Ratio,Psychology,Risk-Taking,Social Support},
month = {mar},
