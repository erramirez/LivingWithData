isbn = {0270-1367 (Print)$\backslash$r0270-1367 (Linking)},
issn = {02701367},
journal = {Research quarterly for exercise and sport},
keywords = {2004,bassett,during the past decade,inexpensive body-worn,measurement,motion sensors for measurement,of physical activity,pedometers are simple and,reliability,step counts,tudor-locke,validity},
number = {3},
pages = {445--453},
pmid = {19791630},
title = {{How many days are enough? A study of 365 days of pedometer monitoring.}},
