@article{Gabriel2011,
abstract = {PURPOSE:: To characterize accelerometer-derived estimates of physical inactivity collected over 5 consecutive weeks in middle-aged women. METHODS:: Data were obtained from 63 participants (95.5{\%}) enrolled in the Evaluation of Physical Activity Measures in Middle-Aged Women (PAW) Study. Inactive time (min/d) was estimated as the sum of activity counts <100 and inactive-to-active transitions were defined as an interruption in which a period of inactivity was immediately followed by a minute or more above 100 counts. Repeated measures ANOVA using PROC MIXED (SAS/STAT software, v. 9.2) were used to describe hourly, daily, and weekly variation in estimates of physical inactivity. RESULTS:: Participants were 52.7 ± 5.5 years, 85.7{\%} non-Hispanic white, 63.5{\%} post-menopausal, with a BMI of 26.7 ± 5.1 kg/m. Inactive time graduallyincreased as the day continued, particularly on weekend days. When compared to weekdays, average inactive time was lower on Saturday and Sunday (all p<.01 except for Saturday vs. Monday p<.10); Saturdays were not significantly different from Sundays. Breaks in inactive time were significantly lower on Sunday when compared to weekdays and Saturday (all p<.05) and fewer breaks were noted on Saturday when compared to Wednesday and Friday (both p<.01). After adjustment for total wear time or inactive time, most day-to-day differences were attenuated. Week-by-week differences in physical inactivity estimates were also not statistically significant. CONCLUSION:: Results of this study suggest that inactive time increases as the day continues and that daily physical inactivity estimates are more stable after 1) adjustment for wear time or 2) when averaged over the week. Researchers should carefully consider the intended application of physical inactivity estimates prior to data collection and processing, analysis, and final data reporting.},
author = {Gabriel, Kelley Pettee and McClain, James J and High, Robin R and Schmid, Kendra K and Whitfield, Geoffrey P and Ainsworth, Barbara E},
doi = {10.1249/MSS.0b013e318229056e},
file = {:Users/eramirez/Documents/Mendeley Desktop/Patterns of Accelerometer-Derived Estimates of Inactivity in Middle-Aged Women. - 2011 - Gabriel et al.pdf:pdf},
isbn = {5123912525},
issn = {1530-0315},
journal = {Medicine and science in sports and exercise},
month = {jun},
number = {June},
pmid = {21685812},
title = {{Patterns of Accelerometer-Derived Estimates of Inactivity in Middle-Aged Women.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21685812},
year = {2011}
}
@article{Margolis2013,
abstract = {Despite significant gains by pediatric collaborative improvement networks, the overall US system of chronic illness care does not work well. A new paradigm is needed: a Collaborative Chronic Care Network (C3N). A C3N is a network-based production system that harnesses the collective intelligence of patients, clinicians, and researchers and distributes the production of knowledge, information, and know-how over large groups of people, dramatically accelerating the discovery process. A C3N is a platform of "operating systems" on which interconnected processes and interventions are designed, tested, and implemented. The social operating system is facilitated by community building, engaging all stakeholders and their expertise, and providing multiple ways to participate. Standard progress measures and a robust information technology infrastructure enable the technical operating system to reduce unwanted variation and adopt advances more rapidly. A structured approach to innovation design provides a scientific operating system or "laboratory" for what works and how to make it work. Data support testing and research on multiple levels: comparative effectiveness research for populations, evaluating care delivery processes at the care center level, and N-of-1 trials and other methods to select the best treatment of individual patient circumstances. Methods to reduce transactional costs to participate include a Federated IRB Model in which centers rely on a protocol approved at 1 central institutional review board and a "commons framework" for organizational copyright and intellectual property concerns. A fully realized C3N represents a discontinuous leap to a self-developing learning health system capable of producing a qualitatively different approach to improving health.},
author = {Margolis, Peter A and Peterson, Laura E and Seid, Michael},
doi = {10.1542/peds.2012-3786J},
issn = {1098-4275},
journal = {Pediatrics},
keywords = {Adolescent,Certification,Child,Child Welfare,Child, Preschool,Chronic Disease,Chronic Disease: therapy,Colitis, Ulcerative,Colitis, Ulcerative: therapy,Community Networks,Community Networks: organization {\&} administration,Cooperative Behavior,Crohn Disease,Crohn Disease: therapy,Delivery of Health Care,Delivery of Health Care: organization {\&} administra,Diffusion of Innovation,Ethics Committees, Research,Ethics Committees, Research: organization {\&} admini,Evidence-Based Medicine,Evidence-Based Medicine: organization {\&} administra,Health Policy,Health Services Research,Health Services Research: organization {\&} administr,Humans,Infant,Information Dissemination,Information Systems,Information Systems: organization {\&} administration,Interdisciplinary Communication,Models, Theoretical,Outcome and Process Assessment (Health Care),Outcome and Process Assessment (Health Care): orga,Pediatrics,Pediatrics: education,Pediatrics: organization {\&} administration,Quality Improvement,Quality Improvement: organization {\&} administration,Societies, Medical,Translational Medical Research,Translational Medical Research: organization {\&} adm,United States},
month = {jun},
