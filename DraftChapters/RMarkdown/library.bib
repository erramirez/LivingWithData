issn = {1096-0260},
journal = {Preventive medicine},
keywords = {Intervention studies,Review,Sedentary behaviour,Sitting time,Workplace},
month = {nov},
number = {5},
pages = {352--6},
pmid = {20801153},
publisher = {Elsevier Inc.},
title = {{Are workplace interventions to reduce sitting effective? A systematic review.}},
