keywords = {Adolescent,Adult,Aged,Child,Communication,Female,Health Behavior,Health Education,Health Education: methods,Health Promotion,Health Promotion: methods,Humans,Male,Middle Aged,Patient Acceptance of Health Care,Patient Acceptance of Health Care: psychology,Teaching Materials,United States,meta-analysis,print,tailoring},
mendeley-tags = {meta-analysis,print,tailoring},
