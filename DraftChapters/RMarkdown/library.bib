issn = {0091-7435},
journal = {Preventive medicine},
keywords = {Adult,Aged,Environment Design,Exercise,Female,Health Behavior,Humans,Longitudinal Studies,Male,Middle Aged,Questionnaires,Recreation,Residence Characteristics,Spatial Behavior,Walking},
month = {jun},
