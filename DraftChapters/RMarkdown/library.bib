issn = {1096-0260},
journal = {Preventive medicine},
month = {apr},
number = {4},
pages = {332--4},
pmid = {19463481},
publisher = {Elsevier Inc.},
title = {{Promoting physical activity during school break times: piloting a simple, low cost intervention.}},
