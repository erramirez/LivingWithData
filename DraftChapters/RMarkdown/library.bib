journal = {Journal of paediatrics and child health},
keywords = {Analysis of Variance,Body Weight,Child,Child Behavior,Child Behavior: psychology,Child, Preschool,Cross-Sectional Studies,Family Characteristics,Female,Humans,Life Style,Male,Obesity,Obesity: epidemiology,Obesity: etiology,Poverty,Prevalence,Risk Factors,Television,Victoria,Victoria: epidemiology},
