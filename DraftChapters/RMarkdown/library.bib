journal = {Health education {\&} behavior : the official publication of the Society for Public Health Education},
keywords = {BCT,Cell Phones,Exercise,Health Behavior,Humans,Regression Analysis,Software,apps,background,theory},
mendeley-tags = {BCT,apps,background,theory},
month = {apr},
