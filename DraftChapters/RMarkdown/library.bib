@article{Chau2011,
abstract = {PURPOSE:: Sitting at work is an emerging occupational health risk. Few instruments designed for use in population-based research measure occupational sitting and standing as distinct behaviours. This study aimed to develop and validate a brief measure of occupational sitting and physical activity. METHODS:: A convenience sample (N=99, 61{\%} female) was recruited from two medium-sized workplaces and by word-of-mouth in Sydney, Australia. Participants completed the newly developed Occupational Sitting and Physical Activity Questionnaire (OSPAQ) and a modified version of the MONICA Optional Study on Physical Activity Questionnaire (modified MOSPA-Q) twice, one week apart. Participants also wore an Actigraph accelerometer for the seven days in between the test and retest. Analyses determined test-retest reliability with intraclass correlation coefficients (ICC) and assessed criterion validity against accelerometers using Spearman's rho. RESULTS:: The test-retest ICCs for occupational sitting, standing and walking for OSPAQ ranged from 0.73-0.90, while that for the modified MOSPA-Q ranged from 0.54-0.89. Comparison of sitting measures with accelerometers showed higher Spearman correlations for the OSPAQ (r=0.65) than for the modified MOSPA-Q (r=0.52). Criterion validity correlations for occupational standing and walking measures were comparable for both instruments with accelerometers (standing: r=0.49; walking: r=0.27-0.29). CONCLUSION:: The OSPAQ has excellent test-retest reliability and moderate validity for estimating time spent sitting and standing at work and is comparable to existing occupational physical activity measures for assessing time spent walking at work. The OSPAQ brief instrument measures sitting and standing at work as distinct behaviours and would be especially suitable in national health surveys, prospective cohort studies and other studies that are limited by space constraints for questionnaire items.},
author = {Chau, Josephine Y and van der Ploeg, Hidde P and Dunn, Scott and Kurko, John and Bauman, Adrian E},
doi = {10.1249/MSS.0b013e3182251060},
file = {:Users/eramirez/Documents/Mendeley Desktop/Validity of the Occupational Sitting and Physical Activity Questionnaire (OSPAQ). - 2011 - Chau et al.pdf:pdf},
issn = {1530-0315},
journal = {Medicine and science in sports and exercise},
month = {jun},
number = {May},
pmid = {21659903},
title = {{Validity of the Occupational Sitting and Physical Activity Questionnaire (OSPAQ).}},
