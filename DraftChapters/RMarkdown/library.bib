issn = {19366574},
journal = {Disability and Health Journal},
keywords = {activity monitor,step count,walking speed},
month = {dec},
title = {{Accuracy of the FitBit at walking speeds and cadences relevant to clinical rehabilitation populations}},
url = {http://www.sciencedirect.com/science/article/pii/S1936657415001740},
year = {2015}
}
