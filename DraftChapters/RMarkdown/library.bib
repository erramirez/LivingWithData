keywords = {Adult,Age Factors,Aged,Body Weight,Breast Neoplasms,Breast Neoplasms: epidemiology,Breast Neoplasms: prevention {\&} control,Exercise,Female,Follow-Up Studies,Health Surveys,Humans,Incidence,Leisure Activities,Middle Aged,Norway,Norway: epidemiology,Proportional Hazards Models,Risk,Risk Factors,Work},
