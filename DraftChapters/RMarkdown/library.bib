isbn = {0002-8614},
journal = {Journal of the American Geriatrics Society},
number = {10},
pages = {1946--1951},
title = {{Trail-Walking Exercise and Fall Risk Factors in Community-Dwelling Older Adults: Preliminary Results of a Randomized Controlled Trial}},
