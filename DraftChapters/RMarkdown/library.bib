issn = {1524-4539},
journal = {Circulation},
keywords = {Adult,Aged,American Native Continental Ancestry Group,American Native Continental Ancestry Group: statis,Asian Continental Ancestry Group,Asian Continental Ancestry Group: statistics {\&} num,Bangladesh,Bangladesh: ethnology,Body Mass Index,Canada,Canada: epidemiology,China,China: ethnology,Diabetes Mellitus, Type 2,Diabetes Mellitus, Type 2: epidemiology,Dyslipidemias,Dyslipidemias: ethnology,Ethnic Groups,Ethnic Groups: statistics {\&} numerical data,Europe,Europe: ethnology,Female,Glucose,Glucose Tolerance Test,Glucose: metabolism,Humans,India,India: ethnology,Insulin,Insulin Resistance,Insulin: blood,Lipids,Lipids: blood,Male,Middle Aged,Obesity,Obesity: diagnosis,Obesity: ethnology,Overweight,Overweight: ethnology,Pakistan,Pakistan: ethnology,Prevalence,Reference Values,Risk Factors,Sri Lanka,Sri Lanka: ethnology},
