@article{Backonja2012b,
abstract = {Keeping individuals aware of their own health is a global challenge in health care. Observations of Daily Living (ODLs), cues to health that are derived from and personally meaningful to an individual, provide a detailed picture of one's experience of health. Project HealthDesign, an 8-year initiative of the Robert Wood Johnson Foundation, is investigating ODLs and devising innovative ways of tracking them through personal health record deployment in diverse communities and health care settings. Nursing informatics knowledge base and skills, applied to the ODL challenge can accelerate their identification, capture, and interpretation, thus empowering individuals toward meaningful action and facilitating more robust information exchange between individuals and their health care providers.},
