issn = {1479-5868},
journal = {The international journal of behavioral nutrition and physical activity},
keywords = {BCT,Cell Phones,Health Behavior,Health Promotion,Humans,Motivation,Motor Activity,Software},
language = {En},
mendeley-tags = {BCT},
