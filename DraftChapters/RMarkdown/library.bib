journal = {Medicine and science in sports and exercise},
keywords = {Acceleration,Adult,Exercise Test,Exercise Test: instrumentation,Exercise Tolerance,Exercise Tolerance: physiology,Female,Humans,Male,Middle Aged,Monitoring, Ambulatory,Monitoring, Ambulatory: instrumentation,Monitoring, Physiologic,Monitoring, Physiologic: instrumentation,Physical Fitness,Physical Fitness: physiology,Probability,Sampling Studies,Sensitivity and Specificity,Walking,Walking: physiology},
month = {may},
number = {5},
