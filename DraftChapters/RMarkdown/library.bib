@article{Mishra2011,
abstract = {Is the eternal quest for precise information always worthwhile? Our research suggests that, at times, vagueness has its merits. Previous research has demonstrated that people prefer precise information over vague information because it gives them a sense of security and makes their environments more predictable. However, we show that the fuzzy boundaries afforded by vague information can actually help individuals perform better than can precise information. We document these findings across two laboratory studies and one quasi-field study that involved different performance-related contexts (mental acuity, physical strength, and weight loss). We argue that the malleability of vague information allows people to interpret it in the manner they desire, so that they can generate positive response expectancies and, thereby, perform better. The rigidity of precise information discourages desirable interpretations. Hence, on certain occasions, precise information is not as helpful as vague information in boosting performance.},
author = {Mishra, Himanshu and Mishra, Arul and Shiv, Baba},
doi = {10.1177/0956797611407208},
file = {::},
issn = {1467-9280},
journal = {Psychological science},
keywords = {Communication,Games, Experimental,Humans,Intelligence,Motivation,Task Performance and Analysis,Uncertainty,Weight Loss},
month = {jun},
number = {6},
pages = {733--8},
pmid = {21515738},
title = {{In praise of vagueness: malleability of vague information as a performance booster.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21515738},
volume = {22},
year = {2011}
}
