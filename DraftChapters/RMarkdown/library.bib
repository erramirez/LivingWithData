@article{LouiseBush2010,
abstract = {There is a paucity of studies regarding noncurricular physical activity promotion interventions among adolescents, and even less such research pertaining to underserved youth. This article describes the development and implementation of a noncurricular, school-based physical activity promotion program designed for a multiethnic, underserved population of adolescents. The program's impact on leisure-time physical activity (LTPA) and on physical activity enjoyment (PAE) is also presented. The 16-week program, named FunAction, utilizes social marketing principles. Control (n = 90) and intervention (n = 131) students are assessed pre- and postintervention for levels of LTPA and PAE. Results indicate that although the program did not contribute to an increase in LTPA or PAE among intervention group students, participation in the program was elevated. This study offers preliminary evidence that noncurricular physical activity promotion programs that apply social marketing principles can be effective in engaging multiethnic, underserved adolescents in physical activity.},
author = {{Louise Bush}, Paula and Laberge, Suzanne and Laforest, Sophie},
doi = {10.1177/1524839908329117},
