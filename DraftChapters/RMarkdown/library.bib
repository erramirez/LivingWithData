title = {{A qualitative analysis of user experiences with a self-tracker for activity, sleep, and diet.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3961803{\&}tool=pmcentrez{\&}rendertype=abstract},
