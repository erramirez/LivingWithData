issn = {0195-9131},
journal = {Medicine and science in sports and exercise},
keywords = {Acceleration,Adult,Discriminant Analysis,Exercise,Exercise: physiology,Female,Geographic Information Systems,Geographic Information Systems: instrumentation,Humans,Male,Middle Aged,Pilot Projects,Predictive Value of Tests},
