keywords = {Adult,Energy Intake,Energy Metabolism,Energy Metabolism: physiology,Exercise Test,Exercise Test: instrumentation,Female,Humans,Male,Monitoring, Ambulatory,Monitoring, Ambulatory: instrumentation,Running,Walking,Wireless Technology,Wireless Technology: instrumentation,Young Adult},
