@article{Buckle2011,
abstract = {Obesity within today's workforce is increasingly recognized as a challenge for both public health professionals and ergonomists. The two disciplines share a scientific, evidence-based approach to practice and now have the opportunity to address a common health issue, namely obesity. This paper shows, through an overview of the current literature, how and where obesity is impacting on the workplace and how the health of the workforce is being affected. Ergonomics and public health priorities have been mapped and challenges suitable for the application of existing knowledge and further research have been identified. Areas of common endeavour, for example how sedentary work may be exacerbating obesity, have been identified as candidate topics for joint activity by ergonomists and public health professionals.},
author = {Buckle, Peter and Buckle, Jessica},
issn = {1757-9139},
