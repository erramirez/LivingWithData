keywords = {Biomechanics,Humans,Low Back Pain,Low Back Pain: etiology,Low Back Pain: physiopathology,Occupational Diseases,Occupational Diseases: etiology,Occupational Diseases: physiopathology,Posture,Posture: physiology,Risk Factors,Sciatica,Sciatica: etiology,Sciatica: physiopathology,Vibration},
