issn = {1873-2607},
journal = {American journal of preventive medicine},
keywords = {BCT,Cell Phones,Coding,Evidence-Based Medicine,Evidence-Based Medicine: methods,Health Behavior,Humans,Mobile Applications,Motivation,Weight Loss,Weight Reduction Programs,Weight Reduction Programs: methods,strategies,techniques},
mendeley-tags = {BCT,Coding,strategies,techniques},
