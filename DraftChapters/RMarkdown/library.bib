keywords = {Activities of Daily Living,Adult,Humans,Monitoring, Physiologic,Monitoring, Physiologic: instrumentation,Monitoring, Physiologic: standards,Movement,Movement: physiology,Observer Variation,Posture,Posture: physiology,Reproducibility of Results},
