journal = {JAMA : the journal of the American Medical Association},
keywords = {Adult,Body Mass Index,Cross-Sectional Studies,Directive Counseling,Emigration and Immigration,Emigration and Immigration: statistics {\&} numerical,Female,Health Behavior,Humans,Life Style,Male,Middle Aged,Obesity,Obesity: epidemiology,Obesity: ethnology,Obesity: prevention {\&} control,Prevalence,Time Factors,United States,United States: epidemiology},
