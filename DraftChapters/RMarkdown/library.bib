month = {jun},
number = {6},
pages = {733--8},
pmid = {21515738},
title = {{In praise of vagueness: malleability of vague information as a performance booster.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21515738},
volume = {22},
year = {2011}
}
