keywords = {accelerometer,doubly-labeled water,energy expenditure,physical activity recognition},
month = {may},
pages = {1--11},
pmid = {21587199},
publisher = {Nature Publishing Group},
title = {{Advances in physical activity monitoring and lifestyle interventions in obesity: a review.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21587199},
year = {2011}
}
