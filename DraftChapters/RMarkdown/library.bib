issn = {0883-6612},
journal = {Annals of behavioral medicine : a publication of the Society of Behavioral Medicine},
keywords = {Body Mass Index,Child,Choice Behavior,Cohort Studies,Cross-Sectional Studies,Environment Design,Exercise,Exercise: psychology,Female,Humans,Male,Nebraska,Overweight,Regression Analysis,Residence Characteristics,Sex Factors,Social Environment,Socioeconomic Factors,Statistics as Topic,Television},
