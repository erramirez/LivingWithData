@article{Kim2008,
abstract = {This study applied a 6-month educational intervention that used the technology of the short message service (via cellular phones) and the Internet for obese patients with type 2 diabetes. Eighteen patients were randomly assigned to an intervention group and 16 were assigned to a control group (N = 34). Patients in the intervention group were asked to access a web site by using personal cellular phones or computer Internet services to input their blood glucose levels daily. Participants were then sent optimal recommendations via cellular phone and the Internet weekly. After 6 months, the intervention group had a statistically significant decrease in glycosylated hemoglobin, fasting plasma glucose, 2-hour postmeal glucose, and total cholesterol, as compared with the control group.},
