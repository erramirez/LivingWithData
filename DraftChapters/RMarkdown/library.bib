keywords = {Aged,Confounding Factors (Epidemiology),Female,Humans,Incidence,Life Style,Middle Aged,Motor Activity,Ovarian Neoplasms,Ovarian Neoplasms: epidemiology,Postmenopause,Proportional Hazards Models,Prospective Studies,Recreation,Risk Factors,United States,United States: epidemiology},
month = {apr},
number = {8},
pages = {709--16},
pmid = {16495470},
title = {{Recreational physical activity and sedentary behavior in relation to ovarian cancer risk in a large cohort of US women.}},
