keywords = {Adolescent,Adult,Analysis of Variance,Chicago,Child,Female,Florida,Health Behavior,Health Behavior: ethnology,Humans,Income,Leisure Activities,Male,Motor Activity,Observation,Public Facilities,Residence Characteristics,Sex Factors},
