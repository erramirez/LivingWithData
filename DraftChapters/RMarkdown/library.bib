issn = {0195-9131},
journal = {Medicine and science in sports and exercise},
keywords = {Adult,Body Mass Index,Cohort Studies,Female,Humans,Incidence,Life Style,Logistic Models,Male,Mental Disorders,Mental Disorders: epidemiology,Motor Activity,Prospective Studies,Questionnaires,Risk Factors,Spain,Spain: epidemiology},
