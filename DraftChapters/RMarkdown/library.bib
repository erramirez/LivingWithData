file = {:Users/eramirez/Documents/Mendeley Desktop/Feasibility, design and conduct of a pragmatic randomized controlled trial to reduce overweight and obesity in children The electroni(2).pdf:pdf},
issn = {1471-2458},
journal = {BMC public health},
keywords = {Adipose Tissue,Adolescent,Body Mass Index,Child,Diet Records,Exercise,Exercise: psychology,Female,Humans,Male,Monitoring, Physiologic,Monitoring, Physiologic: instrumentation,Motivation,Obesity,Obesity: prevention {\&} control,Obesity: therapy,Overweight,Overweight: prevention {\&} control,Overweight: therapy,Pilot Projects,Video Games,Video Games: psychology},
month = {jan},
