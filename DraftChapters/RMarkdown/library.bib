journal = {BMJ (Clinical research ed.)},
keywords = {Body Mass Index,Child,Child Nutrition Sciences,Child Nutrition Sciences: education,Cross-Over Studies,Diet,England,Exercise,Female,Focus Groups,Food Habits,Growth,Health Promotion,Health Promotion: methods,Humans,Male,Obesity,Obesity: etiology,Obesity: prevention {\&} control,Obesity: psychology,Program Evaluation,Risk Factors,School Health Services,Vegetables},
month = {nov},
